{
  "name": "betimeful-extension",
  "displayName": "Betimeful - Stay Focused",
  "version": "0.0.1",
  "description": "BeTimeFul is a powerful extension designed to enhance productivity and concentration by eliminating distracting news feeds. In today's fast-paced digital world, it's easy to get sidetracked by endless streams of news updates and social media feeds.",
  "author": "Moiz Lokhandwala <moizlokhandwala09@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "@plasmohq/redux-persist": "6.1.0",
    "@plasmohq/storage": "^1.9.0",
    "@reduxjs/toolkit": "2.0.1",
    "axios": "^1.6.7",
    "next": "14.1.0",
    "plasmo": "0.84.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "prettier": "3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting",
      "tabs",
      "favicon",
      "storage",
      "contextMenus",
      "clipboardRead",
      "clipboardWrite",
      "sessions",
      "activeTab",
      "action",
      "webNavigation",
      "identity",
      "identity.email"
    ]
  }
}
